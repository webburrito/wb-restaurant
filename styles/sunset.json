{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "title": "Sunset",
  "settings": {
    "color": {
      "palette": [
        { "name": "Primary Orange", "slug": "primary", "color": "#F97316" },
        { "name": "Secondary Pink", "slug": "secondary", "color": "#EC4899" },
        { "name": "Text Dark", "slug": "text", "color": "#292524" },
        { "name": "White", "slug": "white", "color": "#FFFFFF" },
        { "name": "Black", "slug": "black", "color": "#1C1917" },
        { "name": "Warm Cream", "slug": "cream", "color": "#FFF7ED" },
        { "name": "Light Orange", "slug": "red-light", "color": "#FDBA74" },
        { "name": "Dark Orange", "slug": "red-dark", "color": "#C2410C" },
        { "name": "Light Pink", "slug": "blue-light", "color": "#F9A8D4" },
        { "name": "Dark Pink", "slug": "blue-dark", "color": "#BE185D" },
        { "name": "Peach", "slug": "gray-light", "color": "#FFEDD5" },
        { "name": "Rose", "slug": "gray-medium", "color": "#FCA5A5" }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--cream)",
      "text": "var(--wp--preset--color--text)"
    },
    "elements": {
      "button": {
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--white)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary)"
          }
        }
      }
    }
  }
}
